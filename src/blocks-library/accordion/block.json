{
    "$schema": "https://json.schemastore.org/block.json",
    "apiVersion": 2,
    "name": "gitenberg/accordion",
    "title": "Accordion",
    "category": "text",
    "description": "Vertically collapsing blocks perfect for displaying disclosure content. Made for gitenberg",
    "keywords": [
        "accordion"
    ],
    "version": "1.0.0",
    "textdomain": "gitenberg",
    "attributes": {
        "title": {
            "type": "string"
        },
        "initialOpen": {
            "type": "boolean",
            "default": false
        },
		"tag": {
			"type": "string"
		}
    },
    "supports": {
        "multiple": false,
        "spacing": {
            "margin": true
        },
        "customClassName": false,
        "anchor": false,
        "html": false
    },
    "styles": []
}